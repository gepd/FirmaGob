{
  "compilerOptions": {
    "target": "ES5",
    "module": "commonjs",
    "skipLibCheck": true,
    "sourceMap": false,
    "esModuleInterop": true,
    "removeComments": true,
    "outDir": "dist",
    "baseUrl": ".",
    "declaration": true,
    "allowJs": true
  },
  "include": ["src"],
  "exclude": ["node_modules", "dist"]
}
